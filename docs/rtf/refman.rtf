{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Registration
\par}
{\tc\tcl3 \v Registration}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Extensions;\par
{\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Anonymous API}\par
services.AddHttpApiAnonymous({\cf22 "https://api.example.com"});\par
\par
{\cf20 // Bearer token authentication}\par
services.AddHttpApiWithBearerToken(\par
    {\cf22 "https://api.example.com"},\par
    {\cf22 "your-bearer-token"});\par
\par
{\cf20 // Basic authentication}\par
services.AddHttpApiWithBasicAuth(\par
    {\cf22 "https://api.example.com"},\par
    {\cf22 "username"},\par
    {\cf22 "password"});\par
\par
{\cf20 // API key authentication (header)}\par
services.AddHttpApiWithApiKey(\par
    {\cf22 "https://api.example.com"},\par
    {\cf22 "your-api-key"},\par
    {\cf22 "X-API-Key"},\par
    ApiKeyLocation.Header);\par
\par
{\cf20 // API key authentication (query string)}\par
services.AddHttpApiWithApiKey(\par
    {\cf22 "https://api.example.com"},\par
    {\cf22 "your-api-key"},\par
    {\cf22 "api_key"},\par
    ApiKeyLocation.QueryString);\par
\par
{\cf20 // Client certificate authentication}\par
services.AddHttpApiWithCertificate(\par
    {\cf22 "https://api.example.com"},\par
    {\cf17 new} X509Certificate2({\cf22 "client.pfx"}, {\cf22 "password"}));\par
\par
{\cf20 // OAuth2 client credentials}\par
services.AddHttpApiWithOAuth2(\par
    {\cf22 "https://api.example.com"},\par
    {\cf22 "https://auth.example.com/oauth/token"},\par
    {\cf22 "client-id"},\par
    {\cf22 "client-secret"},\par
    {\cf22 "read write"});\par
\par
{\cf20 // Full configuration}\par
services.AddHttpApi(options =>\par
\{\par
    options.BaseUrl = {\cf22 "https://api.example.com"};\par
    options.AuthenticationMode = ApiAuthenticationMode.BearerToken;\par
    options.BearerToken = {\cf22 "your-token"};\par
    options.Timeout = TimeSpan.FromSeconds(30);\par
    options.MaxRetries = 3;\par
    options.UseExponentialBackoff = {\cf17 true};\par
\});\par
\par
{\cf20 // From configuration}\par
services.AddHttpApi(configuration.GetSection({\cf22 "Api"}));\par
}
\par}
