\par}
{\tc\tcl3 \v FTP/FTPS}
File transfer service for FTP and FTPS (FTP over TLS) protocols.\par
{\bkmkstart AAAAAAAABV}
{\bkmkend AAAAAAAABV}
{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Registration
\par}
{\tc\tcl4 \v Registration}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Extensions;\par
{\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Basic FTP}\par
services.AddFtpFileTransfer(options =>\par
\{\par
    options.Host = {\cf22 "ftp.example.com"};\par
    options.Port = 21;\par
    options.Username = {\cf22 "user"};\par
    options.Password = {\cf22 "password"};\par
    options.Protocol = FileTransferProtocol.Ftp;\par
\});\par
\par
{\cf20 // Secure FTPS}\par
services.AddFtpFileTransfer(options =>\par
\{\par
    options.Host = {\cf22 "ftp.example.com"};\par
    options.Port = 21;\par
    options.Username = {\cf22 "user"};\par
    options.Password = {\cf22 "password"};\par
    options.Protocol = FileTransferProtocol.Ftps;\par
\});\par
\par
{\cf20 // From configuration}\par
services.AddFtpFileTransfer(configuration.GetSection({\cf22 "Ftp"}));\par
}
\par}
{\bkmkstart AAAAAAAABW}
{\bkmkend AAAAAAAABW}
{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Usage
\par}
{\tc\tcl4 \v Usage}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }FileService\par
\{\par
    {\cf17 private} readonly IFileTransferService _ftp;\par
\par
    {\cf17 public} FileService(IFileTransferService ftp)\par
    \{\par
        _ftp = ftp;\par
    \}\par
\par
    {\cf17 public} async Task TransferFilesAsync()\par
    \{\par
        {\cf20 // Upload single file}\par
        await _ftp.UploadOneAsync(\par
            localPath: {\cf22 @"C:\\local\\file.txt"},\par
            remotePath: {\cf22 "/remote/file.txt"},\par
            overwrite: {\cf17 true});\par
\par
        {\cf20 // Download single file}\par
        await _ftp.DownloadOneAsync(\par
            remotePath: {\cf22 "/remote/file.txt"},\par
            localPath: {\cf22 @"C:\\local\\downloaded.txt"});\par
\par
        {\cf20 // Batch upload}\par
        var uploads = {\cf17 new}[]\par
        \{\par
            ({\cf22 @"C:\\local\\file1.txt"}, {\cf22 "/remote/file1.txt"}),\par
            ({\cf22 @"C:\\local\\file2.txt"}, {\cf22 "/remote/file2.txt"})\par
        \};\par
        {\cf18 int} successCount = await _ftp.UploadBatchAsync(uploads);\par
\par
        {\cf20 // Batch download}\par
        var downloads = {\cf17 new}[]\par
        \{\par
            ({\cf22 "/remote/file1.txt"}, {\cf22 @"C:\\local\\file1.txt"}),\par
            ({\cf22 "/remote/file2.txt"}, {\cf22 @"C:\\local\\file2.txt"})\par
        \};\par
        {\cf18 int} downloaded = await _ftp.DownloadBatchAsync(downloads);\par
    \}\par
\}\par
}
\par
{\pard\widctlpar\brdrb\brdrs\brdrw5\brsp20 \adjustright \par}
