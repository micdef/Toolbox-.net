{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Registration
\par}
{\tc\tcl4 \v Registration}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Extensions;\par
{\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Generate key pair}\par
var keyPair = RsaCryptographyService.GenerateKeyPair(RsaKeySize.Rsa2048);\par
\par
{\cf20 // Register with full key pair (encrypt + decrypt)}\par
services.AddRsaCryptography(\par
    keyPair.PublicKey!,\par
    RsaPaddingMode.OaepSha256,\par
    keyPair.PrivateKey);\par
\par
{\cf20 // Register with public key only (encrypt only)}\par
services.AddRsaCryptography(\par
    keyPair.PublicKey!,\par
    RsaPaddingMode.OaepSha256);\par
\par
{\cf20 // Register with X.509 certificate}\par
var cert = {\cf17 new} X509Certificate2({\cf22 "certificate.pfx"}, {\cf22 "password"});\par
services.AddRsaCryptography(cert, RsaPaddingMode.OaepSha256);\par
}
\par}
{\bkmkstart AAAAAAAABQ}
{\bkmkend AAAAAAAABQ}
{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Usage
\par}
{\tc\tcl4 \v Usage}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }AsymmetricService\par
\{\par
    {\cf17 private} readonly ICryptographyService _crypto;\par
\par
    {\cf17 public} AsymmetricService(ICryptographyService crypto)\par
    \{\par
        _crypto = crypto;\par
    \}\par
\par
    {\cf17 public} async Task<({\cf18 string} encrypted, {\cf18 string} decrypted)> ProcessAsync({\cf18 string} data)\par
    \{\par
        {\cf20 // Encrypt with public key}\par
        var encrypted = await _crypto.EncryptAsync(data);\par
\par
        {\cf20 // Decrypt with private key (requires private key)}\par
        var decrypted = await _crypto.DecryptAsync(encrypted);\par
\par
        {\cf19 return} (encrypted, decrypted);\par
    \}\par
\}\par
}
\par}
