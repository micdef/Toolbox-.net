{\bkmkstart AAAAAAAACC}
{\bkmkend AAAAAAAACC}
{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Registration
\par}
{\tc\tcl3 \v Registration}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Extensions;\par
{\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Generic SMTP}\par
services.AddSmtpMailing(options =>\par
\{\par
    options.Host = {\cf22 "smtp.example.com"};\par
    options.Port = 587;\par
    options.SecurityMode = SmtpSecurityMode.StartTls;\par
    options.Username = {\cf22 "user@example.com"};\par
    options.Password = {\cf22 "password"};\par
    options.DefaultFrom = {\cf17 new} EmailAddress({\cf22 "noreply@example.com"}, {\cf22 "My App"});\par
\});\par
\par
{\cf20 // Gmail shortcut}\par
services.AddGmailMailing(\par
    email: {\cf22 "myapp@gmail.com"},\par
    appPassword: {\cf22 "xxxx-xxxx-xxxx-xxxx"});\par
\par
{\cf20 // Outlook/Office 365 shortcut}\par
services.AddOutlookMailing(\par
    email: {\cf22 "myapp@outlook.com"},\par
    password: {\cf22 "password"});\par
\par
{\cf20 // Anonymous relay (internal servers)}\par
services.AddSmtpMailing(options =>\par
\{\par
    options.Host = {\cf22 "mail.internal.local"};\par
    options.Port = 25;\par
    options.SecurityMode = SmtpSecurityMode.None;\par
\});\par
\par
{\cf20 // From configuration}\par
services.AddSmtpMailing(configuration.GetSection({\cf22 "Mailing"}));\par
}
\par}
{\bkmkstart AAAAAAAACD}
{\bkmkend AAAAAAAACD}
{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Usage
\par}
{\tc\tcl3 \v Usage}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }NotificationService\par
\{\par
    {\cf17 private} readonly IMailingService _mailing;\par
\par
    {\cf17 public} NotificationService(IMailingService mailing)\par
    \{\par
        _mailing = mailing;\par
    \}\par
\par
    {\cf17 public} async Task SendWelcomeEmailAsync({\cf18 string} userEmail, {\cf18 string} userName)\par
    \{\par
        var message = {\cf17 new} EmailMessage\par
        \{\par
            From = {\cf17 new} EmailAddress({\cf22 "noreply@example.com"}, {\cf22 "My Application"}),\par
            Subject = {\cf22 "Welcome to Our Service!"},\par
            Body = ${\cf22 "<h1>Welcome, \{userName\}!</h1><p>Thank you for joining us.</p>"},\par
            IsBodyHtml = {\cf17 true},\par
            To = \{ {\cf17 new} EmailAddress(userEmail, userName) \}\par
        \};\par
\par
        await _mailing.SendMailAsync(message);\par
    \}\par
\par
    {\cf17 public} async Task SendReportAsync({\cf18 string}[] recipients, {\cf18 byte}[] pdfReport)\par
    \{\par
        var message = {\cf17 new} EmailMessage\par
        \{\par
            From = {\cf17 new} EmailAddress({\cf22 "reports@example.com"}, {\cf22 "Report System"}),\par
            Subject = {\cf22 "Monthly Report"},\par
            Body = {\cf22 "Please find the monthly report attached."},\par
            IsBodyHtml = {\cf17 false},\par
            Attachments =\par
            \{\par
                EmailAttachment.FromBytes(pdfReport, {\cf22 "report.pdf"}, {\cf22 "application/pdf"})\par
            \}\par
        \};\par
\par
        {\cf20 // Multiple recipients}\par
        {\cf19 foreach} (var recipient {\cf19 in} recipients)\par
        \{\par
            message.To.Add(recipient);\par
        \}\par
\par
        await _mailing.SendMailAsync(message);\par
    \}\par
\par
    {\cf17 public} async Task SendBccEmailAsync({\cf18 string}[] bccRecipients)\par
    \{\par
        {\cf20 // Email with BCC only (no visible recipients)}\par
        var message = {\cf17 new} EmailMessage\par
        \{\par
            From = {\cf17 new} EmailAddress({\cf22 "newsletter@example.com"}),\par
            Subject = {\cf22 "Newsletter"},\par
            Body = {\cf22 "<h1>Monthly Newsletter</h1>"},\par
            IsBodyHtml = {\cf17 true}\par
        \};\par
\par
        {\cf19 foreach} (var bcc {\cf19 in} bccRecipients)\par
        \{\par
            message.Bcc.Add(bcc);\par
        \}\par
\par
        await _mailing.SendMailAsync(message);\par
    \}\par
\}\par
}
\par}
