{\tc\tcl4 \v Usage with Options}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Authenticate with options}\par
var authOptions = {\cf17 new} LdapAuthenticationOptions\par
\{\par
    Mode = LdapAuthenticationMode.Simple,\par
    Username = {\cf22 "john.doe"},\par
    Password = {\cf22 "password"},\par
    IncludeGroups = {\cf17 true},\par
    IncludeClaims = {\cf17 true},\par
    Timeout = TimeSpan.FromSeconds(30)\par
\};\par
\par
var result = await ldapService.AuthenticateAsync(authOptions);\par
\par
{\cf19 if} (result.IsAuthenticated)\par
\{\par
    Console.WriteLine(${\cf22 "Authenticated: \{result.Username\}"});\par
    Console.WriteLine(${\cf22 "DN: \{result.UserDistinguishedName\}"});\par
    Console.WriteLine(${\cf22 "Groups: \{string.Join("}, {\cf22 ", result.Groups ?? [])\}"});\par
\}\par
{\cf19 else}\par
\{\par
    Console.WriteLine(${\cf22 "Failed: \{result.ErrorMessage\} (\{result.ErrorCode\})"});\par
\}\par
}
\par}
