{\tc\tcl4 \v Group and Computer Operations}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf20 // Get group by name}\par
var group = await _ldap.GetGroupByNameAsync({\cf22 "Developers"});\par
\par
{\cf20 // Get group members with pagination}\par
var members = await _ldap.GetGroupMembersAsync({\cf22 "CN=Developers,OU=Groups,DC=example,DC=com"}, page: 1, pageSize: 50);\par
\par
{\cf20 // Get computer by name}\par
var computer = await _ldap.GetComputerByNameAsync({\cf22 "SERVER01"});\par
\par
{\cf20 // Search servers}\par
var criteria = LdapComputerSearchCriteria.Create()\par
    .WithOperatingSystem({\cf22 "Windows Server*"})\par
    .EnabledOnly();\par
var servers = await _ldap.SearchComputersAsync(criteria, page: 1, pageSize: 25);\par
}
\par}
