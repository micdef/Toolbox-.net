{\tc\tcl4 \v Usage}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }SecureFileService\par
\{\par
    {\cf17 private} readonly IFileTransferService _sftp;\par
\par
    {\cf17 public} SecureFileService(IFileTransferService sftp)\par
    \{\par
        _sftp = sftp;\par
    \}\par
\par
    {\cf17 public} async Task TransferAsync(CancellationToken ct)\par
    \{\par
        {\cf20 // Upload with cancellation support}\par
        await _sftp.UploadOneAsync(\par
            {\cf22 @"C:\\data\\report.pdf"},\par
            {\cf22 "/uploads/report.pdf"},\par
            overwrite: {\cf17 true},\par
            ct);\par
\par
        {\cf20 // Download}\par
        await _sftp.DownloadOneAsync(\par
            {\cf22 "/downloads/data.csv"},\par
            {\cf22 @"C:\\data\\data.csv"},\par
            cancellationToken: ct);\par
    \}\par
\}\par
}
\par}
