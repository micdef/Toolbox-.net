{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Registration
\par}
{\tc\tcl4 \v Registration}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Extensions;\par
{\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Using current Windows credentials}\par
services.AddActiveDirectory(options =>\par
\{\par
    options.Domain = {\cf22 "corp.example.com"};\par
    options.UseCurrentCredentials = {\cf17 true};\par
    options.UseSsl = {\cf17 true};\par
\});\par
\par
{\cf20 // Using explicit credentials}\par
services.AddActiveDirectory(options =>\par
\{\par
    options.Domain = {\cf22 "corp.example.com"};\par
    options.Server = {\cf22 "dc01.corp.example.com"};\par
    options.Port = 636;\par
    options.UseSsl = {\cf17 true};\par
    options.Username = {\cf22 "CORP\\\\serviceaccount"};\par
    options.Password = {\cf22 "password"};\par
\});\par
\par
{\cf20 // From configuration}\par
services.AddActiveDirectory(configuration.GetSection({\cf22 "Toolbox:Ldap:ActiveDirectory"}));\par
}
\par}
