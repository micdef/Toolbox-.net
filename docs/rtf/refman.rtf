\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Extensions;\par
{\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Password authentication}\par
services.AddSftpFileTransfer(options =>\par
\{\par
    options.Host = {\cf22 "sftp.example.com"};\par
    options.Port = 22;\par
    options.Username = {\cf22 "user"};\par
    options.Password = {\cf22 "password"};\par
\});\par
\par
{\cf20 // Private key authentication}\par
services.AddSftpFileTransfer(options =>\par
\{\par
    options.Host = {\cf22 "sftp.example.com"};\par
    options.Port = 22;\par
    options.Username = {\cf22 "user"};\par
    options.PrivateKeyPath = {\cf22 @"C:\\keys\\id_rsa"};\par
    options.PrivateKeyPassphrase = {\cf22 "passphrase"}; {\cf20 // Optional}\par
\});\par
\par
{\cf20 // Private key from string}\par
services.AddSftpFileTransfer(options =>\par
\{\par
    options.Host = {\cf22 "sftp.example.com"};\par
    options.Username = {\cf22 "user"};\par
    options.PrivateKeyContent = {\cf22 "-----BEGIN RSA PRIVATE KEY-----\\n..."};\par
\});\par
}
