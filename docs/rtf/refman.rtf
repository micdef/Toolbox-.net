\par
{\cf20 // Register with X.509 certificate}\par
var cert = {\cf17 new} X509Certificate2({\cf22 "certificate.pfx"}, {\cf22 "password"});\par
services.AddRsaCryptography(cert, RsaPaddingMode.OaepSha256);\par
}
\par}
