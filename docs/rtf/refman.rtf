\par
{\cf20 // Generate key pair}\par
var keyPair = RsaCryptographyService.GenerateKeyPair(RsaKeySize.Rsa2048);\par
\par
{\cf20 // Register with full key pair (encrypt + decrypt)}\par
services.AddRsaCryptography(\par
    keyPair.PublicKey!,\par
