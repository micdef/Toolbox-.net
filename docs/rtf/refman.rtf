{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Registration
\par}
{\tc\tcl4 \v Registration}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Extensions;\par
{\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Client secret authentication}\par
services.AddAzureAd(options =>\par
\{\par
    options.TenantId = {\cf22 "your-tenant-id"};\par
    options.ClientId = {\cf22 "your-client-id"};\par
    options.ClientSecret = {\cf22 "your-client-secret"};\par
    options.AuthenticationMode = AzureAdAuthMode.ClientSecret;\par
\});\par
\par
{\cf20 // Managed identity (Azure-hosted apps)}\par
services.AddAzureAdWithManagedIdentity(\par
    tenantId: {\cf22 "your-tenant-id"},\par
    clientId: {\cf22 "your-client-id"});\par
\par
{\cf20 // Certificate authentication}\par
services.AddAzureAd(options =>\par
\{\par
    options.TenantId = {\cf22 "your-tenant-id"};\par
    options.ClientId = {\cf22 "your-client-id"};\par
    options.CertificatePath = {\cf22 "/path/to/cert.pfx"};\par
    options.CertificatePassword = {\cf22 "password"};\par
    options.AuthenticationMode = AzureAdAuthMode.Certificate;\par
\});\par
\par
{\cf20 // From configuration}\par
services.AddAzureAd(configuration.GetSection({\cf22 "Toolbox:Ldap:AzureAd"}));\par
}
\par}
