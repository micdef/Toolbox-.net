{\tc\tcl3 \v Advanced Usage}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }AdvancedApiService\par
\{\par
    {\cf17 private} readonly IApiService _api;\par
\par
    {\cf17 public} AdvancedApiService(IApiService api)\par
    \{\par
        _api = api;\par
    \}\par
\par
    {\cf17 public} async Task<SearchResult> SearchAsync({\cf18 string} query, {\cf18 int} page)\par
    \{\par
        {\cf20 // Request with query parameters and headers}\par
        var request = ApiRequest.Get({\cf22 "/search"})\par
            .WithQuery({\cf22 "q"}, query)\par
            .WithQuery({\cf22 "page"}, page.ToString())\par
            .WithHeader({\cf22 "X-Request-ID"}, Guid.NewGuid().ToString())\par
            .WithTimeout(TimeSpan.FromSeconds(60));\par
\par
        var response = await _api.SendAsync(request);\par
\par
        {\cf19 if} (response.IsSuccess)\par
        \{\par
            {\cf19 return} response.Deserialize<SearchResult>()!;\par
        \}\par
\par
        {\cf19 if} (response.IsClientError)\par
        \{\par
            {\cf19 throw} {\cf17 new} InvalidOperationException(${\cf22 "Client error: \{response.ReasonPhrase\}"});\par
        \}\par
\par
        {\cf19 throw} {\cf17 new} HttpRequestException(${\cf22 "Server error: \{response.StatusCode\}"});\par
    \}\par
\par
    {\cf17 public} async Task UploadFileAsync({\cf18 byte}[] fileData, {\cf18 string} fileName)\par
    \{\par
        {\cf20 // Binary content}\par
        var request = ApiRequest.Post({\cf22 "/upload"})\par
            .SetBinaryContent(fileData, {\cf22 "application/octet-stream"})\par
            .WithHeader({\cf22 "X-File-Name"}, fileName);\par
\par
        await _api.SendAsync(request);\par
    \}\par
\par
    {\cf17 public} async Task SubmitFormAsync(Dictionary<string, string> formData)\par
    \{\par
        {\cf20 // Form URL-encoded content}\par
        var request = ApiRequest.Post({\cf22 "/form"})\par
            .SetFormContent(formData);\par
\par
        await _api.SendAsync(request);\par
    \}\par
\}\par
}
\par}
