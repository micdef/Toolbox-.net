{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Synchronous Disposal
\par}
{\tc\tcl3 \v Synchronous Disposal}
For services with synchronous cleanup:\par
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Base;\par
\par
{\cf17 public} {\cf17 class }MyService : BaseDisposableService\par
\{\par
    {\cf17 private} readonly SomeResource _resource;\par
\par
    {\cf17 public} MyService(ILogger<MyService> logger)\par
        : base({\cf22 "MyService"}, logger)\par
    \{\par
        _resource = {\cf17 new} SomeResource();\par
    \}\par
\par
    {\cf17 public} {\cf18 void} DoWork()\par
    \{\par
        ThrowIfDisposed();\par
\par
        {\cf17 using }var activity = StartActivity();\par
        var sw = Stopwatch.StartNew();\par
\par
        _resource.Process();\par
\par
        RecordOperation({\cf22 "DoWork"}, sw.ElapsedMilliseconds);\par
    \}\par
\par
    {\cf17 protected} {\cf17 override} {\cf18 void} Dispose({\cf18 bool} disposing)\par
    \{\par
        {\cf19 if} (disposing)\par
        \{\par
            _resource?.Dispose();\par
        \}\par
        base.Dispose(disposing);\par
    \}\par
\}\par
}
\par}
{\bkmkstart AAAAAAAAIR}
{\bkmkend AAAAAAAAIR}
{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Asynchronous Disposal
\par}
{\tc\tcl3 \v Asynchronous Disposal}
For services with async resources:\par
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Base;\par
\par
{\cf17 public} {\cf17 class }MyAsyncService : BaseAsyncDisposableService\par
\{\par
    {\cf17 private} readonly Stream _stream;\par
\par
    {\cf17 public} MyAsyncService(ILogger<MyAsyncService> logger)\par
        : base({\cf22 "MyAsyncService"}, logger)\par
    \{\par
        _stream = {\cf17 new} FileStream({\cf22 "data.bin"}, FileMode.OpenOrCreate);\par
    \}\par
\par
    {\cf17 public} async Task ProcessAsync(CancellationToken ct = {\cf19 default})\par
    \{\par
        ThrowIfDisposed();\par
\par
        {\cf17 using }var activity = StartActivity();\par
        var sw = Stopwatch.StartNew();\par
\par
        await _stream.WriteAsync(data, ct);\par
\par
        RecordOperation({\cf22 "ProcessAsync"}, sw.ElapsedMilliseconds);\par
    \}\par
\par
    {\cf17 protected} {\cf17 override} async ValueTask DisposeAsyncCore(CancellationToken ct)\par
    \{\par
        await _stream.DisposeAsync();\par
    \}\par
\}\par
}
\par
{\pard\widctlpar\brdrb\brdrs\brdrw5\brsp20 \adjustright \par}
