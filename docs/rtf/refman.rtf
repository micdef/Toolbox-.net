\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }AzureUserService\par
\{\par
    {\cf17 private} readonly ILdapService _azureAd;\par
\par
    {\cf17 public} AzureUserService(ILdapService azureAd)\par
    \{\par
        _azureAd = azureAd;\par
    \}\par
\par
    {\cf17 public} async Task<LdapUser?> GetUserAsync({\cf18 string} email)\par
    \{\par
        {\cf19 return} await _azureAd.GetUserByEmailAsync(email);\par
    \}\par
\par
    {\cf17 public} async Task<IEnumerable<LdapUser>> SearchByDepartmentAsync({\cf18 string} department)\par
    \{\par
        {\cf20 // Azure AD uses OData filter syntax}\par
        {\cf19 return} await _azureAd.SearchUsersAsync(${\cf22 "department eq '\{department\}'"}, maxResults: 50);\par
    \}\par
\par
    {\cf17 public} async Task<IEnumerable<LdapGroup>> GetSecurityGroupsAsync()\par
    \{\par
        {\cf19 return} await _azureAd.SearchGroupsAsync({\cf22 "securityEnabled eq true"}, maxResults: 100);\par
    \}\par
\}\par
}
