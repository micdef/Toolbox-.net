{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Usage
\par}
{\tc\tcl3 \v Usage}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }DataService\par
\{\par
    {\cf17 private} readonly IApiService _api;\par
\par
    {\cf17 public} DataService(IApiService api)\par
    \{\par
        _api = api;\par
    \}\par
\par
    {\cf17 public} async Task<User?> GetUserAsync({\cf18 int} {\cf18 id})\par
    \{\par
        {\cf20 // Simple GET request}\par
        var request = ApiRequest.Get(${\cf22 "/users/\{id\}"});\par
        var response = await _api.SendAsync(request);\par
\par
        response.EnsureSuccess();\par
        {\cf19 return} response.Deserialize<User>();\par
    \}\par
\par
    {\cf17 public} async Task<User?> CreateUserAsync(User user)\par
    \{\par
        {\cf20 // POST with JSON body}\par
        var request = ApiRequest.Post({\cf22 "/users"}, user);\par
        {\cf19 return} await _api.SendAsync<User>(request);\par
    \}\par
\par
    {\cf17 public} async Task UpdateUserAsync({\cf18 int} {\cf18 id}, User user)\par
    \{\par
        {\cf20 // PUT with JSON body}\par
        var request = ApiRequest.Put(${\cf22 "/users/\{id\}"}, user);\par
        await _api.SendAsync(request);\par
    \}\par
\par
    {\cf17 public} async Task PatchUserAsync({\cf18 int} {\cf18 id}, {\cf18 object} partialData)\par
    \{\par
        {\cf20 // PATCH with JSON body}\par
        var request = ApiRequest.Patch(${\cf22 "/users/\{id\}"}, partialData);\par
        await _api.SendAsync(request);\par
    \}\par
\par
    {\cf17 public} async Task DeleteUserAsync({\cf18 int} {\cf18 id})\par
    \{\par
        {\cf20 // DELETE request}\par
        var request = ApiRequest.Delete(${\cf22 "/users/\{id\}"});\par
        await _api.SendAsync(request);\par
    \}\par
\}\par
}
\par}
