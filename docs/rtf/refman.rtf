{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Automatic Token Refresh
\par}
{\tc\tcl3 \v Automatic Token Refresh}
The token refresh service automatically refreshes tokens before they expire.\par
{\bkmkstart AAAAAAAAEH}
{\bkmkend AAAAAAAAEH}
{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Configuration
\par}
{\tc\tcl4 \v Configuration}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid services.AddSsoServices(sso =>\par
\{\par
    sso.EnableAutoRefresh = {\cf17 true};\par
    sso.RefreshThreshold = 0.8;          {\cf20 // Refresh at 80% of lifetime}\par
    sso.RefreshCheckInterval = TimeSpan.FromMinutes(1);\par
    sso.MaxRefreshRetries = 3;\par
    sso.RefreshRetryDelay = TimeSpan.FromSeconds(5);\par
    sso.UseExponentialBackoff = {\cf17 true};\par
\});\par
}
\par}
{\bkmkstart AAAAAAAAEI}
{\bkmkend AAAAAAAAEI}
{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Manual Refresh
\par}
{\tc\tcl4 \v Manual Refresh}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }TokenService\par
\{\par
    {\cf17 private} readonly ITokenRefreshService _refreshService;\par
    {\cf17 private} readonly ISsoSessionManager _sessionManager;\par
\par
    {\cf17 public} TokenService(ITokenRefreshService refreshService, ISsoSessionManager sessionManager)\par
    \{\par
        _refreshService = refreshService;\par
        _sessionManager = sessionManager;\par
    \}\par
\par
    {\cf17 public} async Task<SsoSession?> RefreshNowAsync({\cf18 string} sessionId)\par
    \{\par
        {\cf20 // Immediate refresh}\par
        {\cf19 return} await _refreshService.RefreshNowAsync(sessionId);\par
    \}\par
\par
    {\cf17 public} async Task<int> RefreshAllPendingAsync()\par
    \{\par
        {\cf20 // Refresh all sessions that need it}\par
        {\cf19 return} await _refreshService.RefreshAllPendingAsync();\par
    \}\par
\par
    {\cf17 public} {\cf18 void} CheckStatus()\par
    \{\par
        Console.WriteLine(${\cf22 "Service running: \{_refreshService.IsRunning\}"});\par
        Console.WriteLine(${\cf22 "Registered sessions: \{_refreshService.RegisteredSessionCount\}"});\par
        Console.WriteLine(${\cf22 "Successful refreshes: \{_refreshService.SuccessfulRefreshCount\}"});\par
        Console.WriteLine(${\cf22 "Failed refreshes: \{_refreshService.FailedRefreshCount\}"});\par
        Console.WriteLine(${\cf22 "Last check: \{_refreshService.LastCheckTime\}"});\par
        Console.WriteLine(${\cf22 "Next check: \{_refreshService.NextCheckTime\}"});\par
    \}\par
\}\par
}
\par}
{\bkmkstart AAAAAAAAEJ}
{\bkmkend AAAAAAAAEJ}
{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Refresh Events
\par}
{\tc\tcl4 \v Refresh Events}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }RefreshEventHandler\par
\{\par
    {\cf17 public} RefreshEventHandler(ITokenRefreshService refreshService)\par
    \{\par
        refreshService.RefreshNeeded += OnRefreshNeeded;\par
        refreshService.RefreshCompleted += OnRefreshCompleted;\par
        refreshService.RefreshFailed += OnRefreshFailed;\par
    \}\par
\par
    {\cf17 private} {\cf18 void} OnRefreshNeeded({\cf18 object}? sender, TokenRefreshNeededEventArgs e)\par
    \{\par
        Console.WriteLine(${\cf22 "Refresh needed for \{e.Session.SessionId\}"});\par
        Console.WriteLine(${\cf22 "  Elapsed: \{e.LifetimeElapsedPercent:P0\}"});\par
        Console.WriteLine(${\cf22 "  Time to expiry: \{e.TimeToExpiry\}"});\par
    \}\par
\par
    {\cf17 private} {\cf18 void} OnRefreshCompleted({\cf18 object}? sender, TokenRefreshCompletedEventArgs e)\par
    \{\par
        Console.WriteLine(${\cf22 "Refresh completed for \{e.SessionId\}"});\par
        Console.WriteLine(${\cf22 "  New expiry: \{e.NewExpiresAt\}"});\par
        Console.WriteLine(${\cf22 "  Duration: \{e.RefreshDuration.TotalMilliseconds\}ms"});\par
    \}\par
\par
    {\cf17 private} {\cf18 void} OnRefreshFailed({\cf18 object}? sender, TokenRefreshFailedEventArgs e)\par
    \{\par
        Console.WriteLine(${\cf22 "Refresh failed for \{e.SessionId\}"});\par
        Console.WriteLine(${\cf22 "  Error: \{e.Exception.Message\}"});\par
        Console.WriteLine(${\cf22 "  Retry: \{e.RetryAttempt\}/\{e.MaxRetries\}"});\par
        Console.WriteLine(${\cf22 "  Will retry: \{e.WillRetry\}"});\par
    \}\par
\}\par
}
\par}
\par}
