\par}
{\tc\tcl3 \v Email with Inline Images}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid var logoBytes = File.ReadAllBytes({\cf22 "logo.png"});\par
\par
var message = {\cf17 new} EmailMessage\par
\{\par
    From = {\cf17 new} EmailAddress({\cf22 "noreply@example.com"}),\par
    Subject = {\cf22 "Email with Logo"},\par
    Body = {\cf22 @"}\par
{\cf22         <html>}\par
{\cf22         <body>}\par
{\cf22             <img src='cid:company-logo' alt='Logo' />}\par
{\cf22             <h1>Welcome!</h1>}\par
{\cf22         </body>}\par
{\cf22         </html>"},\par
    IsBodyHtml = {\cf17 true},\par
    To = \{ {\cf22 "recipient@example.com"} \},\par
    Attachments =\par
    \{\par
        EmailAttachment.CreateInline(logoBytes, {\cf22 "logo.png"}, {\cf22 "company-logo"})\par
    \}\par
\};\par
\par
await _mailing.SendMailAsync(message);\par
}
\par}
