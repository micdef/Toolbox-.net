{\tc\tcl4 \v Registration}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Extensions;\par
{\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Generate new key and IV}\par
var (key, iv) = AesCryptographyService.GenerateKey(AesKeySize.Aes256);\par
\par
{\cf20 // Register with generated key}\par
services.AddAesCryptography(key, iv);\par
\par
{\cf20 // Or with key size enum}\par
