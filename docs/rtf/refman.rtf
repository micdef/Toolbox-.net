{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Base64 Encoding
\par}
{\tc\tcl3 \v Base64 Encoding}
Base64 encoding/decoding service for text obfuscation (not encryption).\par
{\bkmkstart AAAAAAAABI}
{\bkmkend AAAAAAAABI}
{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Registration
\par}
{\tc\tcl4 \v Registration}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Extensions;\par
{\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Standard Base64}\par
services.AddBase64Cryptography();\par
\par
{\cf20 // URL-safe Base64}\par
services.AddBase64Cryptography(Base64EncodingTable.UrlSafe);\par
\par
{\cf20 // With full options}\par
services.AddBase64Cryptography(options =>\par
\{\par
    options.EncodingTable = Base64EncodingTable.UrlSafe;\par
    options.IncludePadding = {\cf17 false};\par
\});\par
}
\par}
{\bkmkstart AAAAAAAABJ}
{\bkmkend AAAAAAAABJ}
{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Usage
\par}
{\tc\tcl4 \v Usage}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }MyService\par
\{\par
    {\cf17 private} readonly ICryptographyService _crypto;\par
\par
    {\cf17 public} MyService(ICryptographyService crypto)\par
    \{\par
        _crypto = crypto;\par
    \}\par
\par
    {\cf17 public} async Task ProcessAsync()\par
    \{\par
        {\cf20 // Encode}\par
        var encoded = _crypto.Encrypt({\cf22 "Hello, World!"});\par
        {\cf20 // Result: "SGVsbG8sIFdvcmxkIQ=="}\par
\par
        {\cf20 // Decode}\par
        var decoded = _crypto.Decrypt(encoded);\par
        {\cf20 // Result: "Hello, World!"}\par
\par
        {\cf20 // Async versions}\par
        var encodedAsync = await _crypto.EncryptAsync({\cf22 "Hello"});\par
        var decodedAsync = await _crypto.DecryptAsync(encodedAsync);\par
    \}\par
\}\par
}
\par
{\pard\widctlpar\brdrb\brdrs\brdrw5\brsp20 \adjustright \par}
\par}
\par}
