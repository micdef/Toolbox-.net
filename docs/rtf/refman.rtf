{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Usage
\par}
{\tc\tcl4 \v Usage}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }UserService\par
\{\par
    {\cf17 private} readonly ILdapService _ldap;\par
\par
    {\cf17 public} UserService(ILdapService ldap)\par
    \{\par
        _ldap = ldap;\par
    \}\par
\par
    {\cf17 public} async Task<LdapUser?> FindUserAsync({\cf18 string} username)\par
    \{\par
        var user = await _ldap.GetUserByUsernameAsync(username);\par
\par
        {\cf19 if} (user != {\cf17 null})\par
        \{\par
            Console.WriteLine(${\cf22 "Found: \{user.DisplayName\} (\{user.Email\})"});\par
            Console.WriteLine(${\cf22 "Department: \{user.Department\}"});\par
        \}\par
\par
        {\cf19 return} user;\par
    \}\par
\par
    {\cf17 public} async Task<bool> ValidateUserAsync({\cf18 string} username, {\cf18 string} password)\par
    \{\par
        {\cf19 return} await _ldap.ValidateCredentialsAsync(username, password);\par
    \}\par
\par
    {\cf17 public} async Task<PagedResult<LdapUser>> SearchUsersAsync({\cf18 string} department)\par
    \{\par
        var criteria = LdapSearchCriteria.Create()\par
            .WithDepartment(department)\par
            .EnabledOnly();\par
\par
        {\cf19 return} await _ldap.SearchUsersAsync(criteria, page: 1, pageSize: 25);\par
    \}\par
\}\par
}
\par}
