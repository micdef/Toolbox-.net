{\tc\tcl4 \v Usage}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }SecureService\par
\{\par
    {\cf17 private} readonly ICryptographyService _crypto;\par
\par
    {\cf17 public} SecureService(ICryptographyService crypto)\par
    \{\par
        _crypto = crypto;\par
    \}\par
\par
    {\cf17 public} async Task<string> EncryptDataAsync({\cf18 string} sensitiveData)\par
    \{\par
        {\cf20 // Encrypt (returns Base64-encoded ciphertext)}\par
        var encrypted = await _crypto.EncryptAsync(sensitiveData);\par
\par
        {\cf20 // Decrypt}\par
        var decrypted = await _crypto.DecryptAsync(encrypted);\par
\par
        {\cf19 return} encrypted;\par
    \}\par
\}\par
}
\par}
