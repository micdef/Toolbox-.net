\par}
{\tc\tcl4 \v Usage}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }CredentialService\par
\{\par
    {\cf17 private} readonly ICredentialStore _credentialStore;\par
\par
    {\cf17 public} CredentialService(ICredentialStore credentialStore)\par
    \{\par
        _credentialStore = credentialStore;\par
    \}\par
\par
    {\cf17 public} async Task StoreTokenAsync({\cf18 string} userId, {\cf18 string} accessToken, {\cf18 string} refreshToken)\par
    \{\par
        var credential = {\cf17 new} SsoCredential\par
        \{\par
            UserId = userId,\par
            Type = CredentialType.AccessToken,\par
            AccessToken = accessToken,\par
            RefreshToken = refreshToken,\par
            ExpiresAt = DateTimeOffset.UtcNow.AddHours(1),\par
            DirectoryType = LdapDirectoryType.ActiveDirectory\par
        \};\par
\par
        await _credentialStore.StoreCredentialAsync(${\cf22 "token:\{userId\}"}, credential);\par
    \}\par
\par
    {\cf17 public} async Task<string?> GetTokenAsync({\cf18 string} userId)\par
    \{\par
        var credential = await _credentialStore.GetCredentialAsync(${\cf22 "token:\{userId\}"});\par
        {\cf19 return} credential?.AccessToken;\par
    \}\par
\par
    {\cf17 public} async Task RemoveTokenAsync({\cf18 string} userId)\par
    \{\par
        await _credentialStore.RemoveCredentialAsync(${\cf22 "token:\{userId\}"});\par
    \}\par
\par
    {\cf17 public} async Task<IReadOnlyList<SsoCredential>> GetUserCredentialsAsync({\cf18 string} userId)\par
    \{\par
        {\cf19 return} await _credentialStore.GetUserCredentialsAsync(userId);\par
    \}\par
\par
    {\cf17 public} async Task CleanupExpiredAsync()\par
    \{\par
        var removed = await _credentialStore.CleanupExpiredAsync();\par
        Console.WriteLine(${\cf22 "Removed \{removed\} expired credentials"});\par
    \}\par
\}\par
}
\par}
