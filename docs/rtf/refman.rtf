    Subject = {\cf22 "Monthly Report"},\par
    Body = {\cf22 "<h1>Report</h1><p>See attached.</p>"},\par
    IsBodyHtml = {\cf17 true},\par
    To = \{ {\cf22 "recipient@example.com"} \},\par
    Cc = \{ {\cf17 new} EmailAddress({\cf22 "manager@example.com"}, {\cf22 "Manager"}) \},\par
