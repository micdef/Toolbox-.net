{{\pard\plain \s2\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs28\kerning28\cgrid 
SSO Services
\par}
{\tc\tcl2 \v SSO Services}
Single Sign-On services for session management, credential storage, and automatic token refresh.\par
{\bkmkstart AAAAAAAADY}
{\bkmkend AAAAAAAADY}
{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Registration
\par}
{\tc\tcl3 \v Registration}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 using }Toolbox.Core.Extensions;\par
{\cf17 using }Toolbox.Core.Options;\par
\par
{\cf20 // Basic SSO services}\par
services.AddSsoServices();\par
\par
{\cf20 // With custom configuration}\par
services.AddSsoServices(\par
    sso =>\par
    \{\par
        sso.DefaultSessionDuration = TimeSpan.FromHours(8);\par
        sso.MaxSessionDuration = TimeSpan.FromDays(7);\par
        sso.EnableAutoRefresh = {\cf17 true};\par
        sso.RefreshThreshold = 0.8; {\cf20 // Refresh at 80% of lifetime}\par
        sso.MaxSessionsPerUser = 5;\par
        sso.PersistSessions = {\cf17 true};\par
    \},\par
    credStore =>\par
    \{\par
        credStore.Provider = CredentialStoreProvider.Auto; {\cf20 // Auto-detect platform}\par
        credStore.ApplicationName = {\cf22 "MyApp"};\par
    \});\par
\par
{\cf20 // From configuration}\par
services.AddSsoServices(configuration);\par
}
\par}
{\bkmkstart AAAAAAAADZ}
{\bkmkend AAAAAAAADZ}
{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Session Management
\par}
{\tc\tcl3 \v Session Management}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }AuthController\par
\{\par
    {\cf17 private} readonly ISsoSessionManager _sessionManager;\par
    {\cf17 private} readonly ILdapService _ldapService;\par
\par
    {\cf17 public} AuthController(ISsoSessionManager sessionManager, ILdapService ldapService)\par
    \{\par
        _sessionManager = sessionManager;\par
        _ldapService = ldapService;\par
    \}\par
\par
    {\cf17 public} async Task<SsoSession> LoginAsync({\cf18 string} username, {\cf18 string} password)\par
    \{\par
        {\cf20 // Authenticate with LDAP}\par
        var authResult = await _ldapService.AuthenticateAsync({\cf17 new} LdapAuthenticationOptions\par
        \{\par
            Mode = LdapAuthenticationMode.Simple,\par
            Username = username,\par
            Password = password,\par
            IncludeGroups = {\cf17 true}\par
        \});\par
\par
        {\cf19 if} (!authResult.IsAuthenticated)\par
            {\cf19 throw} {\cf17 new} AuthenticationException(authResult.ErrorMessage);\par
\par
        {\cf20 // Create SSO session}\par
        {\cf19 return} await _sessionManager.CreateSessionAsync(authResult, _ldapService);\par
    \}\par
\par
    {\cf17 public} async Task<SsoSession> LoginWithDeviceBindingAsync(\par
        {\cf18 string} username,\par
        {\cf18 string} password,\par
        {\cf18 string} deviceId,\par
        {\cf18 string} ipAddress,\par
        {\cf18 string} userAgent)\par
    \{\par
        var authResult = await _ldapService.AuthenticateAsync({\cf17 new} LdapAuthenticationOptions\par
        \{\par
            Mode = LdapAuthenticationMode.Simple,\par
            Username = username,\par
            Password = password\par
        \});\par
\par
        {\cf19 if} (!authResult.IsAuthenticated)\par
            {\cf19 throw} {\cf17 new} AuthenticationException(authResult.ErrorMessage);\par
\par
        {\cf20 // Create session with device binding}\par
        {\cf19 return} await _sessionManager.CreateSessionAsync(\par
            authResult,\par
            _ldapService,\par
            deviceId,\par
            ipAddress,\par
            userAgent);\par
    \}\par
\par
    {\cf17 public} async Task<bool> ValidateSessionAsync({\cf18 string} sessionId)\par
    \{\par
        var result = await _sessionManager.ValidateSessionAsync(sessionId);\par
        {\cf19 return} result.IsValid;\par
    \}\par
\par
    {\cf17 public} async Task LogoutAsync({\cf18 string} sessionId)\par
    \{\par
        await _sessionManager.RevokeSessionAsync(sessionId);\par
    \}\par
\par
    {\cf17 public} async Task LogoutAllDevicesAsync({\cf18 string} userId)\par
    \{\par
        await _sessionManager.RevokeAllUserSessionsAsync(userId);\par
    \}\par
\par
    {\cf17 public} async Task LogoutOtherDevicesAsync({\cf18 string} userId, {\cf18 string} currentSessionId)\par
    \{\par
        await _sessionManager.RevokeOtherSessionsAsync(userId, currentSessionId);\par
    \}\par
\}\par
}
\par}
{\bkmkstart AAAAAAAAEA}
{\bkmkend AAAAAAAAEA}
{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Session Validation
\par}
{\tc\tcl3 \v Session Validation}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }SessionMiddleware\par
\{\par
    {\cf17 private} readonly ISsoSessionManager _sessionManager;\par
\par
    {\cf17 public} SessionMiddleware(ISsoSessionManager sessionManager)\par
    \{\par
        _sessionManager = sessionManager;\par
    \}\par
\par
    {\cf17 public} async Task<SsoSessionValidationResult> ValidateAsync(\par
        {\cf18 string} sessionId,\par
        {\cf18 string}? deviceId = {\cf17 null},\par
        {\cf18 string}? ipAddress = {\cf17 null})\par
    \{\par
        var result = await _sessionManager.ValidateSessionAsync(sessionId, deviceId, ipAddress);\par
\par
        {\cf19 if} (!result.IsValid)\par
        \{\par
            {\cf19 switch} (result.FailureReason)\par
            \{\par
                {\cf19 case} SsoValidationFailureReason.SessionNotFound:\par
                    {\cf20 // Session doesn't exist}\par
                    {\cf19 break};\par
                {\cf19 case} SsoValidationFailureReason.SessionExpired:\par
                    {\cf20 // Session has expired}\par
                    {\cf19 break};\par
                {\cf19 case} SsoValidationFailureReason.SessionRevoked:\par
                    {\cf20 // Session was explicitly revoked}\par
                    {\cf19 break};\par
                {\cf19 case} SsoValidationFailureReason.DeviceMismatch:\par
                    {\cf20 // Request from different device}\par
                    {\cf19 break};\par
                {\cf19 case} SsoValidationFailureReason.IpMismatch:\par
                    {\cf20 // Request from different IP}\par
                    {\cf19 break};\par
            \}\par
        \}\par
\par
        {\cf19 return} result;\par
    \}\par
\}\par
}
\par}
{\bkmkstart AAAAAAAAEB}
{\bkmkend AAAAAAAAEB}
{{\pard\plain \s3\sb240\sa60\keepn\widctlpar\adjustright \b\f1\cgrid 
Session Events
\par}
{\tc\tcl3 \v Session Events}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf17 public} {\cf17 class }SessionEventHandler\par
\{\par
    {\cf17 public} SessionEventHandler(ISsoSessionManager sessionManager)\par
    \{\par
        sessionManager.SessionCreated += OnSessionCreated;\par
        sessionManager.SessionExpiring += OnSessionExpiring;\par
        sessionManager.SessionRefreshed += OnSessionRefreshed;\par
        sessionManager.SessionExpired += OnSessionExpired;\par
        sessionManager.SessionRevoked += OnSessionRevoked;\par
    \}\par
\par
    {\cf17 private} {\cf18 void} OnSessionCreated({\cf18 object}? sender, SsoSessionCreatedEventArgs e)\par
    \{\par
        Console.WriteLine(${\cf22 "Session created: \{e.Session.SessionId\} for \{e.Session.UserId\}"});\par
    \}\par
\par
    {\cf17 private} {\cf18 void} OnSessionExpiring({\cf18 object}? sender, SsoSessionExpiringEventArgs e)\par
    \{\par
        Console.WriteLine(${\cf22 "Session expiring in \{e.TimeToExpiry\}: \{e.Session.SessionId\}"});\par
    \}\par
\par
    {\cf17 private} {\cf18 void} OnSessionRefreshed({\cf18 object}? sender, SsoSessionRefreshedEventArgs e)\par
    \{\par
        Console.WriteLine(${\cf22 "Session refreshed: \{e.Session.SessionId\}, new expiry: \{e.NewExpiresAt\}"});\par
    \}\par
\par
    {\cf17 private} {\cf18 void} OnSessionExpired({\cf18 object}? sender, SsoSessionExpiredEventArgs e)\par
    \{\par
        Console.WriteLine(${\cf22 "Session expired: \{e.SessionId\}"});\par
    \}\par
\par
    {\cf17 private} {\cf18 void} OnSessionRevoked({\cf18 object}? sender, SsoSessionRevokedEventArgs e)\par
    \{\par
        Console.WriteLine(${\cf22 "Session revoked: \{e.SessionId\}, reason: \{e.Reason\}"});\par
    \}\par
\}\par
}
