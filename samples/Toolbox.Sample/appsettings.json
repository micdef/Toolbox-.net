{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Toolbox": {
    "EnableDetailedTelemetry": true,
    "ServicePrefix": "Sample"
